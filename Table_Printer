# Table_Printer_Project

print("Welcome To Table Printer")

num = int(input("Input a number: "))



for i in range(1,11):
   print("\t",num,'x',i,'=',num*i)

print("Thank You!")

que = input("Do you want to continue? : ")

while que == "Yes":

   num = int(input("Input a number: "))

   for i in range(1,11):
      print("\t",num,'x',i,'=',num*i)

   print("Thank You!")
   que1 = input("Do you want to continue? : ")
   if que1=="No":
      print("Thank You! Visit Again")
      break
